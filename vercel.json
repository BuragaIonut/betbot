{
  "builds": [
    {
      "src": "next.config.ts",
      "use": "@vercel/next"
    },
    {
      "src": "api/main.py",
      "use": "@vercel/python@5.0.0"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}